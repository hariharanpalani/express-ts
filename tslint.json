{  
    "rules": {
      "class-name": true,
      "curly": false,
      "cyclomatic-complexity": [
        true,
        10
      ],
      "eofline": false,
      "indent": false,
      "member-ordering": false,
      "no-arg": true,
      "no-construct": true,
      "no-duplicate-variable": true,
      "no-empty": false,
      "no-eval": true,
      "no-trailing-whitespace": false,
      "no-unused-expression": [
        true,
        "allow-fast-null-checks"
      ],
      "no-use-before-declare": true,
      "no-conditional-assignment": true,
      "restrict-plus-operands": false,
      "radix": true,
      "typeof-compare": true,
      "one-line": [
        true,
        "check-open-brace",
        "check-whitespace"
      ],
      "jsx-boolean-value": false,
      "jsx-no-multiline-js": false,
      "jsx-alignment": [
        false
      ],
      "quotemark": [
        true,
        "single",
        "jsx-double"
      ],
      "semicolon": [
        true,
        "always"
      ],
      "trailing-comma": true,
      "triple-equals": [
        true,
        "allow-null-check"
      ],
      "variable-name": [
        true,
        "check-format",
        "allow-leading-underscore",
        "allow-pascal-case"
      ],
      "member-access": false,
      "no-string-throw": true,
      "no-string-literal": true,
      "no-debugger": true,
      "no-magic-numbers": [
        true,
        -1,
        0,
        1,
        2,
        10
      ],
      "whitespace": [
        true,
        "check-branch",
        "check-decl",
        "check-operator",
        "check-module",
        "check-separator",
        "check-type",
        "check-preblock"
      ]
    }
}
  